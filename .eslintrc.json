{
  "parserOptions": {
    "ecmaVersion": 2017,
    "impliedStrict": true
  },
  "env": {
    "node": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "indent": [
      "off"
    ],
    "no-unused-vars": [
      "warn"
    ],
    "no-console": [
      "off"
    ],
    "indent": [
      "off",
      2
    ],
    "linebreak-style": [
      "error",
      "windows"
    ],
    "quotes": [
      "off",
      "double"
    ],
    "semi": [
      "error",
      "always"
    ],
    "linebreak-style": [
      "off"
    ]
  }
}